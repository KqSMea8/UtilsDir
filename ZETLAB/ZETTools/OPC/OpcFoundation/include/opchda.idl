typedef enum tagOPCHDA_AGGREGATE
{
	OPCHDA_NOAGGREGATE = 0,
	OPCHDA_INTERPOLATIVE,
	OPCHDA_TOTAL,
	OPCHDA_AVERAGE,
	OPCHDA_TIMEAVERAGE,
